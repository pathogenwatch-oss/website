{
  "name": "wgsa-middle-end",
  "version": "4.4.2",
  "description": "Web API for WGSA",
  "main": "app.js",
  "engines": {
    "node": ">=6.x"
  },
  "dependencies": {
    "amqp": "0.2.4",
    "async": "^1.4.2",
    "babyparse": "^0.2.1",
    "body-parser": "^1.10.1",
    "bunyan": "^1.3.2",
    "couchbase": "^2.0.12",
    "country-reverse-geocoding": "gitlab:cgps/country-reverse-geocoding",
    "ejs": "^2.5.1",
    "express": "^4.10.7",
    "extend": "^2.0.0",
    "longjohn": "^0.2.4",
    "morgan": "^1.5.1",
    "named-argv": "^0.3.0",
    "node-uuid": "^1.4.2",
    "pusher": "^1.1.0",
    "sanitize-filename": "^1.5.3",
    "serve-favicon": "^2.2.0",
    "swig": "^1.4.2",
    "wgsa-fasta-store": "gitlab:cgps/wgsa-fasta-store#v2.2.0",
    "wgsa-front-end": "gitlab:cgps/wgsa-front-end#v4.6.1"
  },
  "devDependencies": {
    "eslint-config-cgps": "github:imperialcollegelondon/eslint-config-cgps",
    "mocha": "^2.1.0",
    "rewire": "^2.1.4",
    "sinon": "^1.12.2",
    "supertest": "^1.0.1"
  },
  "nodepath": "./src",
  "nodeopts": "",
  "startcmd": "NODE_PATH=$npm_package_nodepath node $npm_package_nodeopts",
  "testcmd": "NODE_PATH=$npm_package_nodepath NODE_ENV='testing' mocha -t 60000 --reporter spec",
  "scripts": {
    "start": "eval $npm_package_startcmd start.js | bunyan -o short",
    "test": "eval $npm_package_testcmd 'test/unit/**/*.test.js'",
    "test-example": "eval $npm_package_testcmd 'test/example.test.js'",
    "e2e": "eval $npm_package_testcmd 'test/e2e/**/*.test.js'",
    "upload": "eval $npm_package_startcmd bin/upload.js",
    "download": "eval $npm_package_startcmd bin/download.js",
    "add-fastas": "eval $npm_package_startcmd bin/add-to-fasta-store.js",
    "service": "eval $npm_package_startcmd microservices/bootstrap.js upload-progress | bunyan -o short",
    "ids": "eval $npm_package_startcmd bin/collection-id-update.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ImperialCollegeLondon/wgst-front.git"
  },
  "author": "Imperial College London",
  "bugs": {
    "url": "https://github.com/ImperialCollegeLondon/wgst-front/issues"
  },
  "homepage": "https://github.com/ImperialCollegeLondon/wgst-front",
  "moduleRoots": [
    "src"
  ]
}
